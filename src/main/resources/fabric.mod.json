{
  "schemaVersion": 1,
  "id": "manhunt",
  "version": "${version}",

  "name": "TWO Speedrunner Manhunt",
  "description": "A Manhunt mod with two speedrunners for Minecraft Fabric",
  "authors": [
    "HadronCollision"
  ],

  "license": "MIT License",
  "icon": "assets/modid/icon.png",

  "environment": "*",
  "entrypoints": {
    "main": [
      "net.kaupenjoe.manhunt.Manhunt"
    ],
    "client": [
      "net.kaupenjoe.manhunt.ManhuntClient"
    ]
  },
  "mixins": [
    "manhunt.mixins.json"
  ],

  "depends": {
    "fabricloader": ">=0.14.6",
    "fabric": "*",
    "minecraft": "~1.19",
    "java": ">=17"
  },
  "suggests": {
    "another-mod": "*"
  }
}
